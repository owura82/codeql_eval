 * [XSI] The symbolic names for file modes for use as values of mode_t
 * shall be defined as described in <sys/stat.h>
 */_filesec *filesec_t_filesec_FILESEC_T/* _FILESEC_T */<sys/_types/_filesec_t.h><sys/_types/_s_ifmt.h><sys/_types/_timespec.h>stat64stat64 *lstat64fstat64statx64_nplstatx64_npfstatx64_npumaskx_npstatx_npstat *mkfifox_npmkdirx_nplstatx_nplchmodlchflagsfstatx_npfchmodx_npfchflagschmodx_npchflagsutimensatconst timespecconst timespec[2]timespec[2]futimensmkdiratfstatatfchmodatumaskmknodstatmkfifomkdirlstatfstatfchmodchmodostat__int64_t[2]long long[2]st_qsparest_lsparest_genst_flagsst_blksizest_blocksst_sizest_birthtimespecst_ctimespecst_mtimespecst_atimespecst_rdevst_gidst_uidst_inost_nlinkst_modest_devUTIME_OMIT-2UTIME_NOWEF_IS_SYNTHETICEF_IS_SPARSEEF_IS_PURGEABLEEF_IS_SYNC_ROOTEF_NO_XATTRSEF_MAY_SHARE_BLOCKSSF_DATALESS0x40000000SF_FIRMLINK0x00800000SF_NOUNLINK0x00100000SF_RESTRICTED0x00080000SF_APPEND0x00040000SF_IMMUTABLE0x00020000SF_ARCHIVED0x00010000SF_SYNTHETIC0xc0000000SF_SETTABLE0x3fff0000SF_SUPPORTED0x009f0000UF_HIDDEN0x00008000UF_DATAVAULT0x00000080UF_TRACKED0x00000040UF_COMPRESSEDUF_OPAQUEUF_APPENDUF_IMMUTABLEUF_NODUMPUF_SETTABLE0x0000ffffS_BLKSIZEDEFFILEMODE(S_IRUSR|S_IWUSR|S_IRGRP|S_IWGRP|S_IROTH|S_IWOTH)ALLPERMS(S_ISUID|S_ISGID|S_ISTXT|S_IRWXU|S_IRWXG|S_IRWXO)ACCESSPERMS(S_IRWXU|S_IRWXG|S_IRWXO)S_TYPEISTMO(buf)S_TYPEISSHM(buf)S_TYPEISSEM(buf)S_TYPEISMQ(buf)S_ISWHT(m)(((m) & S_IFMT) == S_IFWHT)S_ISSOCK(m)(((m) & S_IFMT) == S_IFSOCK)S_ISLNK(m)(((m) & S_IFMT) == S_IFLNK)S_ISREG(m)(((m) & S_IFMT) == S_IFREG)S_ISFIFO(m)(((m) & S_IFMT) == S_IFIFO)S_ISDIR(m)(((m) & S_IFMT) == S_IFDIR)S_ISCHR(m)(((m) & S_IFMT) == S_IFCHR)S_ISBLK(m)(((m) & S_IFMT) == S_IFBLK)st_birthtimest_birthtimespec.tv_secst_ctimest_ctimespec.tv_secst_mtimest_mtimespec.tv_secst_atimest_atimespec.tv_sec__DARWIN_STRUCT_STAT64{ dev_t st_dev; mode_t st_mode; nlink_t st_nlink; __darwin_ino64_t st_ino; uid_t st_uid; gid_t st_gid; dev_t st_rdev; __DARWIN_STRUCT_STAT64_TIMES off_t st_size; blkcnt_t st_blocks; blksize_t st_blksize; __uint32_t st_flags; __uint32_t st_gen; __int32_t st_lspare; __int64_t 