 *//Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/usr/include/mach/task_inspect.htask_inspect_info_ttask_inspect_basic_counts *task_inspect_basic_counts_ttask_inspect_basic_counts_data_ttask_inspect_basic_countstask_inspect_flavorTASK_INSPECT_BASIC_COUNTStask_inspect_flavor_tcyclesinstructionsTASK_INSPECT_BASIC_COUNTS_COUNT(sizeof(struct task_inspect_basic_counts) / sizeof(natural_t))MACH_TASK_INSPECT_H/* !defined(MACH_TASK_INSPECT_H) *//*
