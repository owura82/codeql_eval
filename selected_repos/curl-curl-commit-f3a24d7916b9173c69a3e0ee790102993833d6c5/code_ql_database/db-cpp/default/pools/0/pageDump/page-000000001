 *//Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/usr/include/mach/machine/boolean.h"mach/i386/boolean.h"_MACH_MACHINE_BOOLEAN_H_/* _MACH_MACHINE_BOOLEAN_H_ *//Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/usr/include/mach/boolean.h<mach/machine/boolean.h>_MACH_BOOLEAN_H_ASSEMBLER/* _MACH_BOOLEAN_H_ *//* FALSE *//* TRUE *//*
